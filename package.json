{
  "name": "@tsi-trmorris/vue3-draggable",
  "version": "1.0.0",
  "private": false,
  "description": "simple drag&drop component for vue 3.x",
  "author": {
    "name": "Trevor Morris",
    "email": "trmorris@trans-system.com"
  },
  "type": "module",
  "files": ["dist"],
  "main": "./dist/vue3-draggable.umd.cjs",
  "module": "/.dist/vue3-draggable.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-draggable.es.js",
      "require": "./dist/vue3-draggable.umd.cjs"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build"
  },
  "devDependencies": {
    "@tsconfig/bun": "^1.0.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "core-js": "^3.36.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.21.1",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-css-injected-by-js": "^3.4.0",
    "vue": "^3.4.19",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "vue": "^3.4.19"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "singleAttributePerLine": true
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": "https://github.com/tsi-trmorris/vue3-draggable/issues",
  "keywords": [
    "draggable",
    "vue3",
    "vue",
    "drag",
    "drop",
    "drag and drop"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsi-trmorris/vue3-draggable.git"
  }
}
